project Fanctl_Simulator is
    for Main use ("fanctl.adb", "test.adb");
    for Source_Dirs use (
        "Ada_Drivers_Library/hal/src/**",
        "Ada_Drivers_Library/components/src/**",
        "src/simulator",
        "src");
    for Object_Dir use "obj";
    for Create_Missing_Dirs use "True";

    package Compiler is
        for Switches ("Ada") use ("-g");
    end Compiler;
end Fanctl_Simulator;
