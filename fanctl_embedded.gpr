project Fanctl_Embedded is
    for Runtime ("Ada") use "zfp-feather_stm32f405";
    for Target use "arm-eabi";

    for Main use ("fanctl.adb");
    for Source_Dirs use ("src");
    for Object_Dir use "obj";
    for Create_Missing_Dirs use "True";

    package Compiler is
        for Switches ("Ada") use ("-g");
    end Compiler;
end Fanctl_Embedded;
